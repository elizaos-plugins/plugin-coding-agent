{
  "name": "@elizaos/plugin-coding-agent",
  "version": "0.1.0-next.1",
  "description": "Coding agent orchestration plugin - spawn and manage CLI coding agents via PTY",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "bun": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": "./src/index.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "bun run build.ts",
    "prepublishOnly": "bun run build",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elizaos-plugins/plugin-coding-agent.git"
  },
  "homepage": "https://github.com/elizaos-plugins/plugin-coding-agent#readme",
  "bugs": {
    "url": "https://github.com/elizaos-plugins/plugin-coding-agent/issues"
  },
  "keywords": [
    "elizaos",
    "plugin",
    "coding-agent",
    "pty",
    "swarm",
    "claude-code",
    "aider"
  ],
  "peerDependencies": {
    "@elizaos/core": ">=2.0.0-alpha.0",
    "git-workspace-service": ">=0.3.0",
    "pty-manager": ">=1.8.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "typescript": "^5.9.3"
  },
  "license": "MIT",
  "dependencies": {
    "coding-agent-adapters": "0.8.0",
    "pty-console": "0.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
